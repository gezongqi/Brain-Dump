   "source": [
    "line = input(\"Enter the data points : \")\n",
    "date = list(map(int, line.split()))\n",
    "s = sum(date)\n",
    "mean = s / len(date)\n",
    "t = []\n",
    "for i in date:\n",
    "    t.append((i - mean) ** 2)\n",
    "variance = sum(t) / len(date)\n",
    "print(\"mean : \", mean)\n",
    "print(\"Variance : \", variance)"
   ],
