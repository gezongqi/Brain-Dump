    "def Gram(date):\n",
    "    G = np.zeros((len(date), len(date)))\n",
    "    for i in range(len(date)):\n",
    "        for j in range(len(date)):\n",
    "            G[i][j] = np.dot(date[i], date[j])\n",
    "    return G"
